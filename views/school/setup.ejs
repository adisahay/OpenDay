	<body class="page-container-bg-solid page-header-fixed page-sidebar-closed-hide-logo page-md">
		<!-- BEGIN HEADER -->
		<% include ./partialView/header.ejs %>
		<!-- END HEADER -->
		<!-- BEGIN HEADER & CONTENT DIVIDER -->
		<div class="clearfix"> </div>
		<!-- END HEADER & CONTENT DIVIDER -->
		<!-- BEGIN CONTAINER -->
		<div class="page-container">
			<!-- BEGIN SIDEBAR -->
			<% include ./partialView/sidebar.ejs %>
			<!-- END SIDEBAR -->
			<!-- BEGIN CONTENT -->
			<div class="page-content-wrapper">
				<!-- BEGIN CONTENT BODY -->
				<div class="page-content">
					<!-- BEGIN PAGE BASE CONTENT -->
					<div class="row" style="margin-bottom: 12px;">
						<div class="col-md-5">School Details</div>
					</div>
					<div class="custom-body-content">
						<div class="row">
							<div class="col-md-8 col-xs-12 custom-bc-div" data-bind="foreach: schoolDetails">
								<div>School Name</div>
								<input id="schoolName" style="text-transform: uppercase;" type="text" placeholder="eg. ANGELA ROYE'S CHILD DEVELOPMENT CENTER PRESCHOOL & DAYCARE" required />
							</div>
						</div>
						<div class="row">
							<div class="col-md-2 col-xs-12 custom-bc-div">
								<div>Contact No.</div>
								<input id="contactNo" type="text" placeholder="eg. +91 8088764321" required />
							</div>
							<div class="col-md-2 col-xs-12 custom-bc-div">
								<div>Alternate Contact</div>
								<input id="altContact" type="text" placeholder="eg. +91 8088764321" required />
							</div>
							<div class="col-md-3 col-xs-12 col-md-offset-1 custom-bc-div">
								<div>Email ID</div>
								<input id="emailId" type="email" placeholder="eg. arcdcpd@gmail.com" required />
							</div>
						</div>
						<div class="row">
							<div class="col-md-8 col-xs-12 custom-bc-div">
								<div>Address</div>
								<input id="fulladdress" type="text" placeholder="eg. 5/4, Annaswamy Mudaliar Rd, Rukmani Colony, Sivanchetti Gardens" required />
							</div>
						</div>
						<div class="row">
							<div class="col-md-2 col-xs-12 custom-bc-div">
								<div>City</div>
								<input id="city" type="text" placeholder="eg. Bangalore" required />
							</div>
							<div class="col-md-2 col-xs-12 custom-bc-div">
								<div>State</div>
								<input id="state" type="text" placeholder="eg. Karnataka" required />
							</div>
							<div class="col-md-2 col-xs-12 col-md-offset-1 custom-bc-div">
								<div>Pincode</div>
								<input id="postal_code" type="text" placeholder="eg. 560036" required />
							</div>
						</div>
						<hr class="custom-hr">
						<div class="row">
							<div class="col-md-2 col-xs-12 custom-bc-div">
								<div>School Owner</div>
								<input id="owner" type="text" placeholder="eg. Rohan Kumar" required />
							</div>
							<div class="col-md-2 col-xs-12 custom-bc-div">
								<div>Contact No.</div>
								<input id="ownerContact" type="text" placeholder="eg. +91 8899118877" required />
							</div>
							<div class="col-md-2 col-xs-12 col-md-offset-1 custom-bc-div">
								<div>School Principal</div>
								<input id="principal" type="text" placeholder="eg. Rohan Kumar" required />
							</div>
							<div class="col-md-2 col-xs-12 custom-bc-div">
								<div>Contact No.</div>
								<input id="principalContact" type="text" placeholder="eg. +91 8899118877" required />
							</div>
						</div>
						<div class="custom-body-footer margin-top-15">
							<button type="button" id="saveSchool" class="btn theme-color-bck custom-body-button">SAVE CHANGES</button>
						</div>
					</div>
				</div>
				<!-- END CONTENT BODY -->
			</div>
			<!-- END CONTENT -->
		</div>
		<%if (f == 0) { %>
			<div class="custom-notification-bottom">School Setup is Incomplete</div>
		<% } %>
		<!-- END CONTAINER -->
